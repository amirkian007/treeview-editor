{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"]
    },
    "build:fast": {
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "typecheck": {
      "cache": false,
      "dependsOn": ["^typecheck"]
    }

  },
  "globalDependencies": ["tsconfig.json"]
}
