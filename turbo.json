{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"]
    },
    "build:fast": {
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "typecheck": {
      "cache": false,
      "dependsOn": ["^typecheck"]
    },
    "@treeview-editor/treeview-editor#dev": {
      "dependsOn": [
        "@treeview-editor/folder-scaner#build",
        "@treeview-editor/web-socket#build"
      ]
    }

  },
  "globalDependencies": ["tsconfig.json"]
}
